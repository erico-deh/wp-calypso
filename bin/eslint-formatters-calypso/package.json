{
  "name": "eslint-formatters-calypso",
  "version": "0.0.3",
  "main": "./index.js",
  "bin": {
    "eslines": "./index.js"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint *js lib/*js tests/test*js",
    "test": "./node_modules/.bin/tape tests/test*js | ./node_modules/.bin/faucet",
    "test-cli": "./node_modules/.bin/tape tests/test-cli.js | ./node_modules/.bin/faucet",
    "test-eslines": "./node_modules/.bin/tape tests/test-eslines.js | ./node_modules/.bin/faucet",
    "test-exit-codes": "./node_modules/.bin/tape tests/test-exit-codes.js | ./node_modules/.bin/faucet",
    "test-differ": "./node_modules/.bin/tape tests/test-differ.js | ./node_modules/.bin/faucet",
    "test-parsing-errors": "./node_modules/.bin/tape tests/test-parsing-errors.js | ./node_modules/.bin/faucet"
  },
  "dependencies": {
    "eslint": "2.13.1",
    "optionator": "0.8.1"
  },
  "devDependencies": {
    "tape": "4.6.0",
    "faucet": "0.0.1",
    "eslint-config-wpcalypso": "0.2.0",
    "eslint-plugin-wpcalypso": "1.3.2"
  }
}
